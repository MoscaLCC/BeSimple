        <Button x:Name="button" Content="Concluido" HorizontalAlignment="Left" Height="29" Margin="93,154,0,0" VerticalAlignment="Top" Width="73" Click="button_Click"/>
        <Button x:Name="button1" Content="Sair" HorizontalAlignment="Left" Height="29" Margin="198,154,0,0" VerticalAlignment="Top" Width="74" Click="button1_Click"/>

    </Grid>
</Window>
